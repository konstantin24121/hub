{
  "sourceMaps": true,
  "presets": [
    "react",
    "es2015",
    "stage-0",
    "flow",
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-flow-strip-types",
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "flow-react-proptypes",
      ]
    },
  }
}
