{
  "setupFiles": ["./implements/jestsetup.js"],
  "collectCoverageFrom": [
    "src/tools/utils/**/*.js",
    "src/components/[A-Z]*/[A-Z]*.jsx"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 50,
      "lines": 33,
      "statements": 25
    }
  },
  "moduleNameMapper": {
    "^.+\\.(css|pcss)$": "identity-obj-proxy"
  },
  "snapshotSerializers": [
    "<rootDir>/node_modules/enzyme-to-json/serializer"
  ]
}
